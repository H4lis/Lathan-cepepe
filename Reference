#include <iostream>
using namespace std;

int main() {
	int a = 5;

	cout << "Addres dari a = " << &a << endl;
	cout << "Nilai dari a  = " << a << endl << endl;
	int &b = a;
	cout << "Addres dari b = " << &a << endl;
	cout << "Nilai dari b  = " << b << endl << endl;
	a = 10;
	cout << "Nilai dari a " << a << endl;
	cout << "NIlai dari b " << b << endl;
	b = 20;
	cout << "Nilai dari a " << a << endl;
	cout << "NIlai dari b " << b << endl;


}
